{
  "plugins": ["plugins/markdown", "node_modules/jsdoc-vuejs", "jsdoc/undocumented", "jsdoc/staticFiles"],
  "source": {
    "include": ["./src_docs/README.md", "./src/"],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "(^|\\/|\\\\)__tests__"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "tutorials": "./tutorials/"
  },
  "staticFiles": {
    "include": ["img:src_docs_img","img:tutorials/img"]
  }
}
